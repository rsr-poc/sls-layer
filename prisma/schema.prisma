generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("PRISMA_BINARY_TARGET")
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Message {
  id        Int      @id @default(autoincrement())
  text      String
  createdAt DateTime @default(now())
}
